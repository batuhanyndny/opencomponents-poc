<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - asp</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <script>
        window.oc = window.oc || {};
        oc.conf = oc.conf || {};
        oc.conf.templates = (oc.conf.templates || []).concat(
            [
                {
                    "type": "oc-template-es6",
                    "version": "1.0.1",
                    "externals": []
                },
                {
                    "type": "oc-template-jade",
                    "version": "7.0.0",
                    "externals": [{
                        "global": "jade",
                        "url": "https://unpkg.com/oc-jade-legacy@1.11.1/runtime.js",
                        "name": "jade"
                    }]
                },
                {
                    "type": "oc-template-handlebars",
                    "version": "6.0.19", "externals": [{
                        "global": "Handlebars",
                        "url": "https://unpkg.com/handlebars@4.7.2/dist/handlebars.runtime.min.js",
                        "name": "handlebars"
                    }]
                },
                {
                    "type": "oc-template-react",
                    "version": "2.1.0", "externals": [{
                        "global": ["Object", "assign"],
                        "url": "https://unpkg.com/es6-object-assign@1.1.0/dist/object-assign-auto.min.js",
                        "name": "Object.assign"
                    },
                    {
                        "global": "PropTypes",
                        "url": "https://unpkg.com/prop-types@15.7.2/prop-types.min.js",
                        "name": "prop-types"
                    },
                    {
                        "global": "React",
                        "url": "https://unpkg.com/react@16.9.0/umd/react.production.min.js",
                        "name": "react"
                    },
                    {
                        "global": "ReactDOM",
                        "url": "https://unpkg.com/react-dom@16.9.0/umd/react-dom.production.min.js",
                        "name": "react-dom"
                    }]
                }]);
    </script>
</head>

<body>
    <oc-component href="//localhost:3030/Header/1.0.0/"></oc-component>
    
    @RenderBody()
    
    <footer class="border-top footer text-muted">
        <oc-component href="//localhost:3030/Footer/1.0.0/"></oc-component>
    </footer>

    <script src="http://localhost:3030/oc-client/0.48.18/static/src/oc-client.min.js" type="text/javascript"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>
